{
  "name": "@w3stside/wraptor",
  "version": "0.1.1",
  "description": "ETH > WETH Wrapper service - also supports (beta) ERC20 tokens",
  "main": "lib/index.js",
  "module": "lib-esm/index.js",
  "jsnext:main": "lib-esm/index.js",
  "types": "lib-esm/index.d.ts",
  "sideEffects": false,
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/w3stside/wraptor.git"
  },
  "files": [
    "lib",
    "lib-esm"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/w3stside/wraptor/issues"
  },
  "homepage": "https://github.com/w3stside/wraptor#readme",
  "scripts": {
    "start": "npm run install:examples && npm run start:example-ts",
    "start:example-ts": "npm --prefix ./examples/cra/ start",
    "install:examples": "npm run build && npm --prefix ./examples/cra/ install",
    "build": "rimraf ./lib ./lib-esm && npm run build:commonjs && npm run build:esm",
    "build:commonjs": "tsc",
    "build:esm": "tsc -m es6 -t esnext --outDir lib-esm",
    "lint": "eslint src --ext .ts,.tsx,.js",
    "lint:fix": "eslint --fix . --ext .ts,.tsx,.js",
    "stats": "NODE_ENV=production webpack --mode production --env.stats --profile --json > stats.json && webpack-bundle-analyzer ./stats.json ./dist",
    "test": "MOCK=true jest",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\""
  },
  "author": "W3stside <doohhello@gmail.com>",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-regular-svg-icons": "^5.11.2",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@hot-loader/react-dom": "^16.8.6",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.12.10",
    "styled-components": "^4.3.2",
    "web3-utils": "^1.2.4"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-common-types": "^0.2.25",
    "@types/node": "^12.7.5",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "@types/react-router": "^5.0.3",
    "@types/styled-components": "^4.1.19",
    "@types/web3": "^1.2.2",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "eslint-utils": ">=1.4.1",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "style-loader": "^1.0.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.1.0",
    "typescript": "^3.7.2"
  }
}
